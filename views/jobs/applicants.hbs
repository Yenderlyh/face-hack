<h2>Job detail</h2>
<div>{{title}}</div>
<div>{{company}}</div>
<div>{{type}}</div>
<div>{{description}}</div>
<div>{{salary}}</div>
<div>{{journeyType}}</div>
<div>{{vacancies}}</div>
{{#if applicants}}
    <h3>Applicants</h3>
    <ul>
    {{#each applicants}}
        <li class="container">
            <div>{{email}}</div>
        </li>
    {{/each}}
    </ul>
{{/if}}