<h2>Jobs</h2>
<ul>
{{#each jobs}}
  <li><a href="/jobs/{{this.id}}">{{title}}</a>
    <p>{{company}}</p>
    <p>{{type}}</p>
    <p>{{description}}</p>
    <p>{{salary}}</p>
    <p>{{journeyType}}</p>
    <p>{{vacancies}}</p>
    <form action="/jobs/{{this.id}}/delete" method="POST">
      <input type="submit" value="DELETE">
    </form>
  </li>
{{/each}}
</ul>